{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "ES5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": false,
    "lib": ["es6", "dom"]
  },
  "include": ["src/**/*", "src/custom.d.ts"]
}
